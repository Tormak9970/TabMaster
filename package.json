{
  "name": "tabmaster",
  "version": "1.0.0",
  "description": "Allows for adding/removing/rearranging of library tabs. Part of the EmuDeck Project",
  "scripts": {
    "build": "shx rm -rf dist && python/entrypoint.sh && NODE_ENV=production rollup -c",
    "dev": "shx rm -rf dist && python/entrypoint.sh && NODE_ENV=development rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "dbuild build",
    "start": "dbuild deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmuDeck/TabMaster.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "EmuDeck",
    "deck"
  ],
  "author": "Witherking25 <witherking@withertech.com>",
  "license": "GPL-3",
  "bugs": {
    "url": "https://github.com/EmuDeck/TabMaster/issues"
  },
  "homepage": "https://github.com/EmuDeck/TabMaster#readme",
  "devDependencies": {
    "rollup-sourcemap-path-transform": "^1.0.3",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "@types/lodash": "^4.14.191",
    "deckbuild": "^1.0.9",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.6.1",
    "lodash": "^4.17.21",
    "mobx": "5",
    "react-icons": "^4.4.0",
    "camelcase": "^7.0.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
